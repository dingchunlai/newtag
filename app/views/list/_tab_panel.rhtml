<div class="ltext">
<div id="tab-panel-<%=panel_id%>" class="<%=classname%>">
<ul class="newst">
<%ars = get_about_list(type, keyword, 15)%>
<%if ars.length > 0%>
  <%for ar in ars%>
  <li style="height:26px;">
    <a href="<%=ar["url"]%>" target="_blank" class="grey"><%=ar["title"]%></a>
  </li>
  <%end%>
<%else%>
  <li style="height:26px;color:#808080">
    暂无数据
  </li>
<%end%>
</ul>
</div>
</div>