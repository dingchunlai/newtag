<div id="tab-panel-210">
<!--recommend news with pic-->
<ul class="newst">
  <%ars = get_about_list(1, @keyword, 20)%>
  <%if ars.length > 0%>
    <%for ar in ars%>
    <li style="height:26px;">
      <a href="<%=ar["url"]%>" target="_blank" class="grey"><%=ar["title"]%></a>
    </li>
    <%end%>
  <%else%>
    <li style="height:26px;color:#808080">
      暂无数据
    </li>
  <%end%>
</ul>
</div>