<script type="text/javascript">
    function setFocus2(p, y, o, m) {
        selectLayer2(p, y, o, m);
    }
    function selectLayer2(p, y, o, m) {
        for (var i = 0; i < document.getElementById(y).getElementsByTagName(p).length;) {
            document.getElementById(o + i).style.display = "none";
            i++;
        }
        document.getElementById(o + m).style.display = "block";
    }
    function daohang(m) {
        for (var z = 0; z <= 9;) {
            document.getElementById(z + 1).style.backgroundImage = 'url(http://www.51hejia.com/images/jiaju1123/btn_bg_bk.gif)';
            document.getElementById(z + 1).style.color = '#1a4176';
            z++;
        }
        ;
        document.getElementById(m).style.backgroundImage = 'url(http://www.51hejia.com/images/jiaju1123/btn_bg.gif)';
        document.getElementById(m).style.color = '#fff';
